Ativo DEFINITIONS AUTOMATIC TAGS ::=
BEGIN

Connect ::= SEQUENCE{
    deviceType BOOLEAN,
    deviceInfo PrintableString(SIZE(1..20))
}

Registro ::= SEQUENCE{
    confirmation PrintableString(SIZE(1..12)),
    info PrintableString(SIZE(1..12)) 
}

Data ::= SEQUENCE{
    info PrintableString(SIZE(1..12)),
    payload PrintableString(SIZE(1..256))
}

Ativo ::= SEQUENCE{ 
    id CHOICE{
        connect Connect,
        registro Registro,
        data Data         
    }
}
 
END
