Ativo DEFINITIONS AUTOMATIC TAGS ::=
BEGIN

Iniciate ::= SEQUENCE{
    name PrintableString(SIZE(1..20)),
    atop PrintableString(SIZE(1..20)),
    type INTEGER
}

Data ::= SEQUENCE{
    payload PrintableString(SIZE(1..256))
}

Ativo ::= SEQUENCE{
    id CHOICE{
        iniciate Iniciate,
        data Data    
    }
}
 
END
